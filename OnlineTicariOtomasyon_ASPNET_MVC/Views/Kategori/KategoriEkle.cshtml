@model OnlineTicariOtomasyon_ASPNET_MVC.Models.Siniflar.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<link href="~/Content/style.css" rel="stylesheet" />
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="~/Scripts/SweetAlertEkle.js"></script>
<br />
<h2>Yeni Kategori Sayfası</h2>
<hr />
@using (Html.BeginForm("KategoriEkle", "Kategori", FormMethod.Post))
{
    <div class="form-group">
        <label class="labelAciklama">Kategori Adı</label>
        @Html.TextBoxFor(x => x.KategoriAd, new { @class = "form-control" })
    </div>
    <button id="Button3" runat="server" class="btn btn-primary btn-block" onclick="return conFunction(this);">Kaydet</button>
}

